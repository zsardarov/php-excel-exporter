{
    "name": "zsardarov/php-excel-exporter",
    "description": "A library that provides a way to easily export report data to excel",
    "keywords": [
        "php",
        "excel"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Zurab Sardarov"
        }
    ],
    "require": {
        "box/spout": "^3.1",
        "symfony/http-foundation": "~2.7|~3.0|~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Zsardarov\\ExcelExporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zsardarov\\ExcelExporter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
